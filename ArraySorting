import java.util.Arrays;
public class Main{
    public static void main(String args[])
    {
    int arr[]={22,4,14,10,28,13,44,9,7};
	
	      Arrays.sort(arr);
			      System.out.println();
			      System.out.println("Array After Sorting = ");
			      for(int i=0;i<arr.length;i++)
			      {
				        System.out.print(" "+arr[i]);
			      }
    }
}
